<%- include('./partials/header');  %> 

<h1>Home</h1>
<p><%= startingContent %></p>

<% posts.forEach(post => { %>
    <div class="post">
        <h3><%= post.title %></h3>
        <p><%= post.body.substring(0, 150) + ' ...' %></p>
        <a href="/posts/<%=post._id%>" class="btn btn-info btn-sm">Read More</a>
    </div>
<% }) %>

<%- include('./partials/footer');  %> 
